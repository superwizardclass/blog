{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321/blog",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/src/content/posts"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "posts",
      "previewPath": "'posts'",
      "pageBundle": false,
      "clearEmpty": false,
      "fields": [
        {
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "pubDate",
          "type": "datetime",
          "default": "{{now}}",
          "required": true
        },
        {
          "name": "description",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "tags",
          "type": "list",
          "required": true
        },
        {
          "name": "image",
          "type": "string",
          "single": true,
          "required": false,
          "default": ""
        },
        {
          "name": "publish",
          "type": "boolean",
          "required": true
        },
        {
          "name": "author",
          "type": "string",
          "required": true,
          "default": "dan"
        }
      ]
    }
  ],
  "frontMatter.website.host": "https://superwizardclass.github.io/blog/"
}
